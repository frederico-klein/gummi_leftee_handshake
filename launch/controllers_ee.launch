<launch>
  <!-- This file is specific for handshake ee (end-effector). The eval expression is superfluous and
  it only makes sense to keep it, so that this can be adapted to other end-effectors faster. -->
  <arg name="ee" value="$(env ROS_GUMMI_LEFTEE)"/>
    <rosparam file="$(eval find('gummi_leftee_' + ee) + '/dynamixel/left_forearm_roll.yaml')" command="load"/>
    <node name="left_forearm_roll_controller_spawner" pkg="dynamixel_controllers" type="controller_spawner.py"
          args="--manager=dxl_manager
                --port gummi_d
                left_forearm_roll_controller"
          output="screen"/>

   <rosparam file="$(eval find('gummi_leftee_' + ee) + '/dynamixel/left_hand_close.yaml')" command="load"/>
    <node name="left_hand_close_controller_spawner" pkg="dynamixel_controllers" type="controller_spawner.py"
          args="--manager=dxl_manager
                --port gummi_d
                left_hand_close_controller"
          output="screen"/>

    <rosparam file="$(eval find('gummi_leftee_' + ee) + '/dynamixel/left_wrist_flexor.yaml')" command="load"/>
    <node name="left_wrist_flexor_controller_spawner" pkg="dynamixel_controllers" type="controller_spawner.py"
          args="--manager=dxl_manager
                --port gummi_d
                left_wrist_flexor_controller"
          output="screen"/>

    <rosparam file="$(eval find('gummi_leftee_' + ee) + '/dynamixel/left_wrist_extensor.yaml')" command="load"/>
    <node name="left_wrist_extensor_controller_spawner" pkg="dynamixel_controllers" type="controller_spawner.py"
          args="--manager=dxl_manager
                --port gummi_d
                left_wrist_extensor_controller"
          output="screen"/>

    <rosparam file="$(eval find('gummi_leftee_' + ee) + '/dynamixel/left_wrist_encoder.yaml')" command="load"/>
    <node name="left_wrist_encoder_controller_spawner" pkg="dynamixel_controllers" type="controller_spawner.py"
          args="--manager=dxl_manager
                --port gummi_ae
                left_wrist_encoder_controller"
          output="screen"/>

</launch>
